<h1>Listing elilists</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Subscribers</th>
    <th>Owners</th>
    <th>List type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @elilists.each do |elilist| %>
  <tr>
    <td><%= elilist.name %></td>
    <td><%= elilist.subscribers %></td>
    <td><%= elilist.owners %></td>
    <td><%= elilist.list_type %></td>
    <td><%= link_to 'Show', elilist %></td>
    <td><%= link_to 'Edit', edit_elilist_path(elilist) %></td>
    <td><%= link_to 'Destroy', elilist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Elilist', new_elilist_path %>
